{
  "name": "ein-haus-am-see",
  "version": "1.0.0",
  "description": "Bachelorarbeit \"Ein Haus am See im Browser - Interaktives Finden geeigneter Orte\"",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./public/dist && webpack --mode production",
    "build-watch": "rimraf ./public/dist && webpack --mode development && webpack --watch --mode development",
    "serve": "ts-node-dev src/index.ts",
    "start": "concurrently --kill-others \"npm run build-watch\" \"npm run serve\"",
    "deploy": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelmeckl/BA-Ein-Haus-am-See.git"
  },
