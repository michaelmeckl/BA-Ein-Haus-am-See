{
  "name": "ein-haus-am-see",
  "version": "1.0.0",
  "description": "Bachelorarbeit \"Ein Haus am See im Browser - Interaktives Finden geeigneter Orte\"",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./src/public/dist && webpack",
    "build-watch": "webpack --watch",
    "serve": "ts-node-dev --files -r dotenv/config src",
    "start": "webpack && concurrently --kill-others \"npm run build-watch\" \"npm run serve\"",
    "deploy": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelmeckl/BA-Ein-Haus-am-See.git"
  },
  "keywords": [
    "web mapping",
    "multi-criteria local search",
    "data visualization"
  ],
  "author": "Michael Meckl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelmeckl/BA-Ein-Haus-am-See/issues"
  },
  "homepage": "https://github.com/michaelmeckl/BA-Ein-Haus-am-See#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "query-overpass": "^1.5.5"
  },
  "devDependencies": {
    "@mapbox/geojson-area": "^0.2.2",
    "@types/cors": "^2.8.6",
